{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "moduleResolution": "node",
        "noImplicitAny": true,
        "experimentalDecorators": true,
        "preserveConstEnums": true,
        "outDir": "lib",
        "declaration": true,
        "jsx": "react",
        "lib": [
            "es2015",
            "dom"
        ]
    },
    "exclude": [
        "node_modules",
        "examples",
        "libs"
    ]
}